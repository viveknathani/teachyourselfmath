    "express-session": "^1.18.0",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.3",
    "node-pg-migrate": "^6.2.2",
    "passport": "^0.7.0",
