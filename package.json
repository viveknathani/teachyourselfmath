    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.11",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@bull-board/api": "^5.9.1",
